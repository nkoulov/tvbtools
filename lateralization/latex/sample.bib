@article{chan_temporal_2012,
	title = {The {Temporal} {Experience} of {Pleasure} {Scale} ({TEPS}): {Exploration} and {Confirmation} of {Factor} {Structure} in a {Healthy} {Chinese} {Sample}},
	volume = {7},
