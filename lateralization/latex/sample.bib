}

@article{daianu_breakdown_2013,
	title = {Breakdown of {Brain} {Connectivity} {Between} {Normal} {Aging} and {Alzheimer}'s {Disease}: {A} {Structural} \textit{k} -{Core} {Network} {Analysis}},
