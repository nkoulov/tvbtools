	pages = {64},
	file = {Sidey-Gibbons and Sidey-Gibbons - 2019 - Machine learning in medicine a practical introduc.pdf:C\:\\Users\\Wayne\\Zotero\\storage\\PCXUWLW2\\PCXUWLW2.pdf:application/pdf},
}

