	file = {Liu et al. - 2019 - Using deep Siamese neural networks for detection o.pdf:C\:\\Users\\Wayne\\Zotero\\storage\\J2MYYNSK\\Liu et al. - 2019 - Using deep Siamese neural networks for detection o.pdf:application/pdf},
