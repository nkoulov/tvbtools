	pages = {103--114},
	file = {Hanson et al. - 2009 - Solving the brain synchrony eigenvalue problem co.pdf:C\:\\Users\\Wayne\\Zotero\\storage\\2EDS2WA9\\Hanson et al. - 2009 - Solving the brain synchrony eigenvalue problem co.pdf:application/pdf},
}

