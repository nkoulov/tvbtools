	pages = {10100--10115},
	file = {Chen et al. - 2019 - Decreased inter-hemispheric interactions but incre.pdf:C\:\\Users\\Wayne\\Zotero\\storage\\897L29Z5\\Chen et al. - 2019 - Decreased inter-hemispheric interactions but incre.pdf:application/pdf},
}

