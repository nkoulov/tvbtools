	file = {Špiláková et al. - 2019 - Dissecting social interaction dual-fMRI reveals p.pdf:C\:\\Users\\Wayne\\Zotero\\storage\\XRICYUS5\\Špiláková et al. - 2019 - Dissecting social interaction dual-fMRI reveals p.pdf:application/pdf},
