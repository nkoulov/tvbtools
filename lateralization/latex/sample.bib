	file = {Reuter-Lorenz and Mikels - 2005 - A split-brain model of Alzheimer's disease.pdf:C\:\\Users\\Wayne\\Zotero\\storage\\LGUVN9NA\\Reuter-Lorenz and Mikels - 2005 - A split-brain model of Alzheimer's disease.pdf:application/pdf},
